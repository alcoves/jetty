{
  "name": "foghorn",
  "version": "0.0.0",
  "main": "main.js",
  "targets": {
    "main": false
  },
  "repository": "git@github.com:bkenio/foghorn.git",
  "author": "Brendan Kennedy <brendan@bken.io>",
  "license": "MPL-2.0",
  "contributors": [
    "Brendan Kennedy <brendan@bken.io>"
  ],
  "build": {
    "appId": "com.bken.foghorn",
    "files": [
      "main.js",
      "build/*"
    ],
    "win": {
      "publish": [
        "github"
      ],
      "icon": "./src/public/favicon.ico"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "parcel ./src/public/index.html",
    "start:dev": "cross-env ELECTRON_START_URL=http://localhost:1234 electron .",
    "start": "electron .",
    "lint": "eslint . --ext .ts",
    "build": "parcel build ./src/public/index.html --dist-dir build --public-url ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.2.0",
    "parcel": "^2.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@swc/helpers": "^0.2.14",
    "cross-env": "^7.0.3",
    "electron-updater": "^4.3.9",
    "framer-motion": "^4",
    "peerjs": "^1.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "socket.io-client": "^4.3.2"
  }
}